#!/usr/bin/env php
<?php

include __DIR__.'/../vendor/autoload.php';

use Che\BuildGuild\Console\GuildApp;

$version = '@package_version@';
// Version should be replaced, if not, use unknown
if ($version === '@' . 'package_version' . '@') {
    $version = 'dev';
}

$app = new GuildApp($version);

$app->run();
